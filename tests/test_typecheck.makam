%use "../init".
%open string.

typecheck_test_suite : testsuite.
%testsuite typecheck_test_suite.

parse_tc_file : string -> string -> prop.
parse_tc_file FileName Type :-
    string.readfile FileName FileContents,
    parse_tc FileContents Type.

testcase typecheck_test_suite :-
    parse_tc_file "examples/example1.fgo" "false".
